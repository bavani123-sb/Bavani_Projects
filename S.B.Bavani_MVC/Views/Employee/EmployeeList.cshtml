@model List<EmployeeInfo>

<h1>Employee List</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>User Name</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody id="employeeTableBody">
        @await Html.PartialAsync("~/Views/Shared/_EmployeeList.cshtml", Model)
    </tbody>
</table>

<br />
<a asp-controller="Employee" asp-action="EmployeeForm" class="btn btn-primary">Back to Form</a>
